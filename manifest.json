{
"manifest_version": 3,
"name": "Fenix Discord Tool",
"version": "0.0",
"description": "",
"icons": {
"16": "assets/icon.png",
"48": "assets/icon.png",
"128": "assets/icon.png"
},
"permissions": [
"declarativeNetRequestWithHostAccess",
"scripting",
"activeTab"
],
"host_permissions": ["https://discord.com/*"],
"background": {
"service_worker": "background.js"
},
"content_scripts": [
{
"matches": ["https://discord.com/*"],
"js": ["quest-home.js"],
"run_at": "document_idle"
},
{
"matches": ["https://discord.com/*"],
"js": ["user-agent-override.js"],
"run_at": "document_start",
"world": "MAIN"
}
],
"declarative_net_request": {
"rule_resources": [
{
"id": "ruleset_1",
"enabled": true,
"path": "rules.json"
}
]
}
}